  "bugs": {
    "url": "https://github.com/awmottaz/promise-some/issues"
  },
  "homepage": "https://github.com/awmottaz/promise-some#readme",
  "devDependencies": {
    "ava": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "typescript": "^4.7.4"
  }
}
